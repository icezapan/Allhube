local Tab = Window:NewTab("Setting")
local Section = Tab:NewSection("Jump power")
Section:NewSlider("JumpPower", "Slider to get more jumppower", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)
Section:NewSlider("Speed", "Slider to get more speed", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
    end)
