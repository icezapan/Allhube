--Map: Every Second You Get +1 Jump
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("All Hub", "Synapse")
local Tab = Window:NewTab("Farm")
local Section = Tab:NewSection("Earth Tower")
Section:NewButton("Click", "Click to go end", function()
 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-10242.20703125, 1871.864013671875, 128.54251098632812)
end)
Section:NewToggle("AutoFarm", "Click to Auto Farm", function(state)
_G.AutoFarm = true
while _G.AutoFarm do wait()
    if state then
 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-10242.20703125, 1871.864013671875, 128.54251098632812)
    else
_G.AutoFarm = false
while _G.AutoFarm do wait()
    if state then
 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-9862.9619140625, 8982.228515625, -44.44383239746094)
            end
        end
    end
end
end)
